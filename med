string toLower(const string& str) {
    string lowerStr = str;
    transform(lowerStr.begin(), lowerStr.end(), lowerStr.begin(), 
              [](unsigned char c){ return tolower(c); });
    return lowerStr;
}

vector<string> splitString(const string& input, char delimiter) {
    vector<string> tokens;
    string token;
    istringstream tokenStream(input);
    while (getline(tokenStream, token, delimiter)) {
        if (!token.empty()) {
            tokens.push_back(token);
        }
    }
    return tokens;
}

bool getYesNoResponse(const string& question) {
    while (true) {
        cout << question << " (yes/no): ";
        string response;
        getline(cin, response);
        
        response = toLower(response);
        if (response == "yes" || response == "y") {
            return true;
        } else if (response == "no" || response == "n") {
            return false;
        }
        cout << "Please answer with 'yes' or 'no'.\n";
    }
}

int getNumericResponse(const string& prompt, int min, int max) {
    int value;
    while (true) {
        cout << prompt;
        string input;
        getline(cin, input);
        
        try {
            value = stoi(input);
            if (value >= min && value <= max) {
                return value;
            }
            cout << "Please enter a number between " << min << " and " << max << ".\n";
        } catch (...) {
            cout << "Please enter a valid number.\n";
        }
    }
}

MedicineType getMedicinePreference() {
    cout << "\nPlease choose your preferred medicine type:\n";
    cout << "1. Allopathy (Modern Medicine)\n";
    cout << "2. Ayurvedic (Traditional Indian Medicine)\n";
    cout << "3. Homeopathy\n";
    
    int choice = getNumericResponse("Enter your choice (1-3): ", 1, 3);
    
    switch(choice) {
        case 1: return MedicineType::ALLOPATHY;
        case 2: return MedicineType::AYURVEDIC;
        case 3: return MedicineType::HOMEOPATHY;
        default: return MedicineType::ALLOPATHY;
    }
}

PatientInfo gatherPatientInformation() {
    PatientInfo info;
    
    cout << "\n=== Patient Information ===\n";
    cout << "To provide the best advice, I need some basic information.\n";
    
    cout << "What is your name? ";
    getline(cin, info.name);
    
    info.age = getNumericResponse("What is your age? ", 0, 120);
    
    cout << "What is your gender (M/F/O)? ";
    string gender;
    getline(cin, gender);
    info.gender = !gender.empty() ? toupper(gender[0]) : 'O';
    
    cout << "Do you have any existing medical conditions? ";
    string conditions;
    getline(cin, conditions);
    info.existingConditions = splitString(conditions, ',');
    
    cout << "Are you currently taking any medications? ";
    string meds;
    getline(cin, meds);
    info.currentMedications = splitString(meds, ',');
    
    cout << "Do you have any known allergies? ";
    string allergies;
    getline(cin, allergies);
    info.allergies = splitString(allergies, ',');
    
    info.preferredMedicine = getMedicinePreference();
    
    return info;
}

vector<Symptom> gatherSymptoms() {
    vector<Symptom> symptoms;
    
    cout << "\n=== Symptom Assessment ===\n";
    cout << "Please describe your symptoms in detail.\n";
    cout << "For example: 'I have had a severe headache for 12 hours and mild nausea'\n";
    cout << "Your description: ";
    
    string description;
    getline(cin, description);
    
    vector<string> symptomKeywords = {
        "headache", "fever", "cough", "sore throat", "runny nose",
        "nausea", "vomiting", "diarrhea", "rash", "fatigue",
        "body aches", "chills", "sneezing", "itchy eyes", "stomach pain",
        "shortness of breath", "dizziness", "chest pain", "swelling",
        "jaundice", "joint pain", "stiffness", "loss of appetite",
        "weight loss", "rapid heartbeat", "nervousness", "sweating",
        "tremors", "heat intolerance", "persistent sadness", "loss of interest",
        "sleep disturbances", "blurred vision", "slow healing", "frequent urination",
        "increased thirst", "hunger", "night sweats", "blood in sputum",
        "confusion", "dark urine", "abdominal pain", "phlegm"
    };
    
    for (const string& keyword : symptomKeywords) {
        if (toLower(description).find(toLower(keyword)) != string::npos) {
            Symptom symptom;
            symptom.name = keyword;
            
            if (toLower(description).find("severe") != string::npos) {
                symptom.severity = Severity::SEVERE;
            } else if (toLower(description).find("moderate") != string::npos) {
                symptom.severity = Severity::MODERATE;
            } else {
                symptom.severity = Severity::MILD;
            }
            
            if (toLower(description).find("hour") == string::npos &&
                toLower(description).find("day") == string::npos &&
                toLower(description).find("week") == string::npos) {
                symptom.durationHours = getNumericResponse(
                    "How many hours have you had " + keyword + "? ", 0, 720);
            } else {
                symptom.durationHours = 24;
            }
            
            symptoms.push_back(symptom);
        }
    }
    
    return symptoms;
}

void askFollowUpQuestions(const vector<Symptom>& symptoms, PatientInfo& patient) {
    cout << "\n=== Follow-up Questions ===\n";
    
    for (const Symptom& symptom : symptoms) {
        if (symptom.name == "fever") {
            if (getYesNoResponse("Have you measured your temperature? If yes, was it above 101F (38.3C)?")) {
            }
        }
        
        if (symptom.name == "chest pain") {
            if (getYesNoResponse("Does the chest pain radiate to your arm or jaw?")) {
                cout << "WARNING: This could indicate a heart attack. Please seek emergency care immediately.\n";
            }
        }
        
        if (symptom.name == "shortness of breath") {
            if (getYesNoResponse("Is the shortness of breath worse when lying down?")) {
            }
        }
        
        if (symptom.name == "jaundice") {
            if (getYesNoResponse("Have you noticed dark urine or pale stools?")) {
            }
        }
    }
    
    bool hasRedFlag = any_of(symptoms.begin(), symptoms.end(), [](const Symptom& s) {
        return (s.name == "chest pain" && s.severity == Severity::SEVERE) ||
               (s.name == "difficulty breathing") ||
               (s.name == "severe headache" && s.durationHours < 24) ||
               (s.name == "confusion") ||
               (s.name == "blood in sputum");
    });
    
    if (hasRedFlag) {
        cout << "\nWARNING: You're describing symptoms that may require immediate medical attention.\n";
        cout << "I strongly recommend you seek emergency care or call your local emergency number.\n";
    }
}

vector<pair<Disease, int>> assessConditions(const vector<Symptom>& symptoms, const PatientInfo& patient) {
    vector<pair<Disease, int>> possibleConditions;
    
    for (const auto& diseasePair : diseaseDatabase) {
        const Disease& disease = diseasePair.second;
        int matchScore = 0;
        
        for (const string& primarySymptom : disease.primarySymptoms) {
            auto it = find_if(symptoms.begin(), symptoms.end(), 
                [&primarySymptom](const Symptom& s) { 
                    return s.name == primarySymptom; 
                });
            if (it != symptoms.end()) {
                matchScore += 3;
            }
        }
        
        for (const string& secondarySymptom : disease.secondarySymptoms) {
            auto it = find_if(symptoms.begin(), symptoms.end(), 
                [&secondarySymptom](const Symptom& s) { 
                    return s.name == secondarySymptom; 
                });
            if (it != symptoms.end()) {
                matchScore += 1;
            }
        }
        
        for (const string& riskFactor : disease.riskFactors) {
            if (find(patient.existingConditions.begin(), 
                    patient.existingConditions.end(), riskFactor) != patient.existingConditions.end()) {
                matchScore += 2;
            }
        }
        
        if (matchScore >= 3) { 
            possibleConditions.emplace_back(disease, matchScore);
        }
    }
    
    sort(possibleConditions.begin(), possibleConditions.end(),
        [](const pair<Disease, int>& a, const pair<Disease, int>& b) {
            return a.second > b.second;
        });
    
    return possibleConditions;
}

void displayCondition(const Disease& disease, int rank, MedicineType preferredMedicine) {
    cout << "\n" << rank << ". " << disease.name << " (" << disease.category << ")\n";
    
    cout << "\nPrimary Symptoms:\n";
    for (const string& symptom : disease.primarySymptoms) {
        cout << "- " << symptom << "\n";
    }
    
    if (!disease.secondarySymptoms.empty()) {
        cout << "\nSecondary Symptoms:\n";
        for (const string& symptom : disease.secondarySymptoms) {
            cout << "- " << symptom << "\n";
        }
    }
    
    cout << "\nRecommended Treatments (" << 
        (preferredMedicine == MedicineType::ALLOPATHY ? "Allopathy" : 
         preferredMedicine == MedicineType::AYURVEDIC ? "Ayurvedic" : "Homeopathy") << "):\n";
    
    vector<Medication> treatments;
    switch(preferredMedicine) {
        case MedicineType::ALLOPATHY: treatments = disease.allopathyTreatments; break;
        case MedicineType::AYURVEDIC: treatments = disease.ayurvedicTreatments; break;
        case MedicineType::HOMEOPATHY: treatments = disease.homeopathyTreatments; break;
    }
    
    for (const Medication& med : treatments) {
        cout << "- " << med.name << ": " << med.dosage << " " 
             << med.frequency << " for " << med.duration << "\n";
    }
    
    if (!disease.homeRemedies.empty()) {
        cout << "\nHome Remedies:\n";
        for (const string& remedy : disease.homeRemedies) {
            cout << "- " << remedy << "\n";
        }
    }
    
    cout << "\nPrecautions:\n";
    for (const string& precaution : disease.precautions) {
        cout << "- " << precaution << "\n";
    }
    
    if (!disease.diagnosticTests.empty()) {
        cout << "\nDiagnostic Tests to Confirm:\n";
        for (const string& test : disease.diagnosticTests) {
            cout << "- " << test << "\n";
        }
    }
    
    cout << "\nContagious: " << (disease.contagious ? "Yes" : "No") << "\n";
}

void provideGeneralAdvice(const vector<Symptom>& symptoms) {
    cout << "\n=== General Health Advice ===\n";
    
    if (any_of(symptoms.begin(), symptoms.end(), 
              [](const Symptom& s) { return s.name == "fever"; })) {
        cout << "- For fever: Stay hydrated with water, clear broths, or oral rehydration solutions.\n";
        cout << "- Rest as much as possible to help your body fight the infection.\n";
        cout << "- Use fever reducers like acetaminophen or ibuprofen as directed.\n";
    }
    
    if (any_of(symptoms.begin(), symptoms.end(), 
              [](const Symptom& s) { return s.name == "cough"; })) {
        cout << "- For cough: Try honey (for adults and children over 1 year), warm liquids, or cough drops.\n";
        cout << "- Use a humidifier to moisten the air and soothe irritated airways.\n";
    }
    
    if (any_of(symptoms.begin(), symptoms.end(), 
              [](const Symptom& s) { return s.name == "diarrhea"; })) {
        cout << "- For diarrhea: Drink plenty of fluids to prevent dehydration.\n";
        cout << "- Eat bland foods like bananas, rice, applesauce, and toast (BRAT diet).\n";
        cout << "- Avoid dairy, fatty foods, and caffeine until symptoms improve.\n";
    }
    
    cout << "\nWhen to Seek Medical Attention:\n";
    cout << "- Difficulty breathing or shortness of breath\n";
    cout << "- Persistent pain or pressure in the chest\n";
    cout << "- New confusion or inability to arouse\n";
    cout << "- Bluish lips or face\n";
    cout << "- Severe, persistent symptoms that don't improve with self-care\n";
}

void generateReport(const PatientInfo& patient, const vector<Symptom>& symptoms, 
                   const vector<pair<Disease, int>>& possibleConditions) {
    time_t now = time(nullptr);
    tm* localTime = localtime(&now);
    
    string filename = "medical_report_" + patient.name + ".txt";
    for (char &c : filename) {
        if (c == ' ') c = '_';
    }
    
    ofstream reportFile(filename);
    
    reportFile << "=== COMPREHENSIVE MEDICAL ASSESSMENT REPORT ===\n";
    reportFile << "Generated on: " << put_time(localTime, "%Y-%m-%d %H:%M") << "\n\n";
    
    reportFile << "=== PATIENT INFORMATION ===\n";
    reportFile << "Name: " << patient.name << "\n";
    reportFile << "Age: " << patient.age << "\n";
    reportFile << "Gender: " << patient.gender << "\n";
    reportFile << "Preferred Medicine Type: ";
    switch(patient.preferredMedicine) {
        case MedicineType::ALLOPATHY: reportFile << "Allopathy (Modern Medicine)\n"; break;
        case MedicineType::AYURVEDIC: reportFile << "Ayurvedic (Traditional Indian Medicine)\n"; break;
        case MedicineType::HOMEOPATHY: reportFile << "Homeopathy\n"; break;
    }
    
    reportFile << "\nExisting Medical Conditions:\n";
    if (patient.existingConditions.empty()) {
        reportFile << "None reported\n";
    } else {
        for (const string& condition : patient.existingConditions) {
            reportFile << "- " << condition << "\n";
        }
    }
    
    reportFile << "\nCurrent Medications:\n";
    if (patient.currentMedications.empty()) {
        reportFile << "None reported\n";
    } else {
        for (const string& med : patient.currentMedications) {
            reportFile << "- " << med << "\n";
        }
    }
    
    reportFile << "\nKnown Allergies:\n";
    if (patient.allergies.empty()) {
        reportFile << "None reported\n";
    } else {
        for (const string& allergy : patient.allergies) {
            reportFile << "- " << allergy << "\n";
        }
    }
    
    reportFile << "\n=== SYMPTOMS REPORTED ===\n";
    for (const Symptom& symptom : symptoms) {
        reportFile << "- " << symptom.name << " (";
        switch (symptom.severity) {
            case Severity::MILD: reportFile << "Mild"; break;
            case Severity::MODERATE: reportFile << "Moderate"; break;
            case Severity::SEVERE: reportFile << "Severe"; break;
        }
        reportFile << ", Duration: " << symptom.durationHours << " hours)\n";
    }
    
    if (!possibleConditions.empty()) {
        reportFile << "\n=== POSSIBLE CONDITIONS (BY LIKELIHOOD) ===\n";
        for (size_t i = 0; i < possibleConditions.size(); ++i) {
            const Disease& disease = possibleConditions[i].first;
            reportFile << "\n" << i+1 << ". " << disease.name << " (" << disease.category << ")\n";
            reportFile << "Match Score: " << possibleConditions[i].second << "/10\n";
            
            reportFile << "\nPrimary Symptoms:\n";
            for (const string& symptom : disease.primarySymptoms) {
                reportFile << "- " << symptom << "\n";
            }
            
            if (!disease.secondarySymptoms.empty()) {
                reportFile << "\nSecondary Symptoms:\n";
                for (const string& symptom : disease.secondarySymptoms) {
                    reportFile << "- " << symptom << "\n";
                }
            }
            
            reportFile << "\nRecommended Treatments (" << 
                (patient.preferredMedicine == MedicineType::ALLOPATHY ? "Allopathy" : 
                 patient.preferredMedicine == MedicineType::AYURVEDIC ? "Ayurvedic" : "Homeopathy") << "):\n";
            
            vector<Medication> treatments;
            switch(patient.preferredMedicine) {
                case MedicineType::ALLOPATHY: treatments = disease.allopathyTreatments; break;
                case MedicineType::AYURVEDIC: treatments = disease.ayurvedicTreatments; break;
                case MedicineType::HOMEOPATHY: treatments = disease.homeopathyTreatments; break;
            }
            
            for (const Medication& med : treatments) {
                reportFile << "- " << med.name << ": " << med.dosage << " " 
                         << med.frequency << " for " << med.duration << "\n";
            }

            reportFile << "\nAlternative Treatment Options:\n";
            
            if (patient.preferredMedicine != MedicineType::ALLOPATHY && !disease.allopathyTreatments.empty()) {
                reportFile << "Allopathy Options:\n";
                for (const Medication& med : disease.allopathyTreatments) {
                    reportFile << "- " << med.name << ": " << med.dosage << " " 
                             << med.frequency << " for " << med.duration << "\n";
                }
            }
            
            if (patient.preferredMedicine != MedicineType::AYURVEDIC && !disease.ayurvedicTreatments.empty()) {
                reportFile << "Ayurvedic Options:\n";
                for (const Medication& med : disease.ayurvedicTreatments) {
                    reportFile << "- " << med.name << ": " << med.dosage << " " 
                             << med.frequency << " for " << med.duration << "\n";
                }
            }
            
            if (patient.preferredMedicine != MedicineType::HOMEOPATHY && !disease.homeopathyTreatments.empty()) {
                reportFile << "Homeopathy Options:\n";
                for (const Medication& med : disease.homeopathyTreatments) {
                    reportFile << "- " << med.name << ": " << med.dosage << " " 
                             << med.frequency << " for " << med.duration << "\n";
                }
            }
            
            if (!disease.homeRemedies.empty()) {
                reportFile << "\nHome Remedies:\n";
                for (const string& remedy : disease.homeRemedies) {
                    reportFile << "- " << remedy << "\n";
                }
            }
            
            reportFile << "\nPrecautions:\n";
            for (const string& precaution : disease.precautions) {
                reportFile << "- " << precaution << "\n";
            }
            
            if (!disease.diagnosticTests.empty()) {
                reportFile << "\nDiagnostic Tests to Confirm:\n";
                for (const string& test : disease.diagnosticTests) {
                    reportFile << "- " << test << "\n";
                }
            }
            
            reportFile << "\nContagious: " << (disease.contagious ? "Yes" : "No") << "\n";
            reportFile << "Risk Factors: ";
            for (const string& risk : disease.riskFactors) {
                reportFile << risk << ", ";
            }
            reportFile << "\n";
        }
    } else {
        reportFile << "\nNo specific conditions identified based on reported symptoms.\n";
    }
    
    reportFile << "\n=== GENERAL HEALTH ADVICE ===\n";
    reportFile << "- Maintain proper hydration with water and electrolyte solutions\n";
    reportFile << "- Get adequate rest to support your immune system\n";
    reportFile << "- Wash hands frequently to prevent infection spread\n";
    reportFile << "- Monitor symptoms and seek medical attention if they worsen\n";
    
    reportFile << "\nEMERGENCY WARNING SIGNS (Seek immediate medical care for):\n";
    reportFile << "- Difficulty breathing or shortness of breath\n";
    reportFile << "- Persistent chest pain or pressure\n";
    reportFile << "- New confusion or inability to stay awake\n";
    reportFile << "- Bluish lips or face\n";
    reportFile << "- Severe, unrelenting pain\n";
    
    reportFile << "\n=== DISCLAIMER ===\n";
    reportFile << "This report is generated by an automated system and is not a substitute\n";
    reportFile << "for professional medical advice, diagnosis, or treatment. Always seek\n";
    reportFile << "the advice of your physician or other qualified health provider with\n";
    reportFile << "any questions you may have regarding a medical condition. Never disregard\n";
    reportFile << "professional medical advice or delay seeking it because of information\n";
    reportFile << "in this report.\n";
    
    reportFile << "\nReport generated by: Advanced Medical Diagnosis Assistant\n";
    
    reportFile.close();
    cout << "\nA detailed report has been saved to '" << filename << "'\n";
}

void conductAssessment() {
    PatientInfo patient = gatherPatientInformation();
    vector<Symptom> symptoms = gatherSymptoms();
    askFollowUpQuestions(symptoms, patient);
    
    vector<pair<Disease, int>> possibleConditions = assessConditions(symptoms, patient);
    
    cout << "\n=== Assessment Results ===\n";
    if (!possibleConditions.empty()) {
        cout << "Based on your symptoms, here are possible conditions:\n";
        for (size_t i = 0; i < min(possibleConditions.size(), size_t(3)); ++i) {
            displayCondition(possibleConditions[i].first, i+1, patient.preferredMedicine);
        }
    } else {
        cout << "No specific conditions identified based on your symptoms.\n";
        provideGeneralAdvice(symptoms);
    }
    
    generateReport(patient, symptoms, possibleConditions);
    
    cout << "\nRemember:\n";
    cout << "- This is not a substitute for professional medical advice\n";
    cout << "- If symptoms are severe or worsening, seek medical attention\n";
    cout << "- For emergencies, call your local emergency number immediately\n";
}

int main() {
    cout << "=== Advanced Medical Diagnosis Assistant ===\n";
    cout << "This tool helps assess possible medical conditions based on symptoms.\n";
    cout << "It provides treatment options in Allopathy, Ayurveda, and Homeopathy.\n";
    cout << "NOTE: This does NOT replace professional medical evaluation.\n\n";
    
    bool continueAssessments = true;
    while (continueAssessments) {
        conductAssessment();
        continueAssessments = getYesNoResponse("\nWould you like to perform another assessment?");
    }
    
    cout << "\nThank you for using the Medical Diagnosis Assistant. Stay healthy!\n";
    
    return 0;
}
