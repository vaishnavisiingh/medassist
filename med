#include <iostream>
#include <string>
#include <vector>
#include <map>
#include <set>
#include <algorithm>
#include <cctype>
#include <fstream>
#include <sstream>
#include <ctime>
#include <iomanip>

using namespace std;

enum class Severity { MILD, MODERATE, SEVERE };
enum class MedicineType { ALLOPATHY, AYURVEDIC, HOMEOPATHY };

struct Symptom {
    string name;
    Severity severity;
    int durationHours;
};

struct Medication {
    string name;
    string dosage;
    string frequency;
    string duration;
    MedicineType type;
};

struct Disease {
    string name;
    string category;
    vector<string> primarySymptoms;
    vector<string> secondarySymptoms;
    vector<Medication> allopathyTreatments;
    vector<Medication> ayurvedicTreatments;
    vector<Medication> homeopathyTreatments;
    vector<string> precautions;
    vector<string> homeRemedies;
    vector<string> riskFactors;
    vector<string> diagnosticTests;
    bool contagious;
};

struct PatientInfo {
    string name;
    int age;
    char gender;
    vector<string> existingConditions;
    vector<string> currentMedications;
    vector<string> allergies;
    MedicineType preferredMedicine;
};

